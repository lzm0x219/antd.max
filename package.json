{
  "name": "antd.max",
  "version": "0.0.0-development",
  "description": "🌈 A React business component library powered by ant-design.",
  "keywords": [
    "react",
    "react-components",
    "antd",
    "antd.max",
    "ant-design"
  ],
  "license": "MIT",
  "author": {
    "name": "尾宿君",
    "email": "lzm0x219@gmail.com"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "src",
    "typings"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc --project ./tsconfig.build.json && pnpm api-extractor run --local --verbose",
    "clean": "rimraf dist typings",
    "coverage": "vitest run --coverage",
    "docs:build": "dumi build",
    "docs:dev": "dumi dev",
    "docs:preview": "serve dist",
    "start": "pnpm docs:dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "preversion": "pnpm build",
    "postversion": "git push --follow-tags"
  },
  "lint-staged": {
    "*.{ts,tsx,js,cjs}": [
      "rome format --write",
      "rome check"
    ],
    "*.{md,html,css,json,yml}": "prettier --write",
    "!(*pnpm-lock).yaml": "prettier --write",
    "package.json": "pnpm sort-package-json"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.3.2",
    "antd": "5.1.1",
    "dayjs": "1.11.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "dumi": "2.0.18",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "prettier-plugin-packagejson": "2.3.0",
    "rimraf": "3.0.2",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.4",
    "vitest": "0.26.3"
  },
  "peerDependencies": {
    "@ant-design/cssinjs": "*",
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "antd": "5.x",
    "dayjs": "1.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "peerDependenciesMeta": {
    "@ant-design/cssinjs": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@testing-library/dom"
      ],
      "allowAny": [
        "react",
        "react-dom"
      ]
    }
  }
}
