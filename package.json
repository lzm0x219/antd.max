{
  "name": "antd.max",
  "version": "0.0.0-development",
  "description": "🌈 A React business component library powered by ant-design.",
  "keywords": [
    "react",
    "react-components",
    "antd",
    "antd.max",
    "ant-design"
  ],
  "license": "MIT",
  "author": {
    "name": "尾宿君",
    "email": "lzm0x219@gmail.com"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "src",
    "typings"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc --project ./tsconfig.build.json && pnpm api-extractor run --local --verbose",
    "clean": "rimraf dist typings",
    "coverage": "vitest run --coverage",
    "docs:build": "dumi build",
    "docs:dev": "dumi dev",
    "docs:preview": "serve dist",
    "start": "pnpm docs:dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "preversion": "pnpm build",
    "postversion": "git push --follow-tags"
  },
  "lint-staged": {
    ".{ts,tsx,cjs}": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,md}": "prettier --write",
    "package.json": "pnpm sort-package-json"
  },
  "dependencies": {
    "antd": "5.0.0-alpha.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@umijs/plugin-esbuild": "1.4.2",
    "dumi": "1.1.48",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.3.0",
    "rimraf": "3.0.2",
    "serve": "14.0.1",
    "sort-package-json": "1.57.0",
    "typescript": "4.8.4",
    "vitest": "0.23.4"
  },
  "peerDependencies": {
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "antd": "5.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@testing-library/dom",
        "@umijs/preset-dumi",
        "react-router",
        "umi",
        "webpack"
      ],
      "allowAny": [
        "react",
        "react-dom"
      ]
    }
  }
}
